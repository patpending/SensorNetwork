package org.dongargon.util;

public class WirelessSensorConfig {
	private String type;
	private String ID;
	private String niceName;
	private int cosmFeed = -1;
	private int cosmDataSet = -1;
	
	public int getCosmFeed() {
		return cosmFeed;
	}
	public void setCosmFeed(int cosmFeed) {
		this.cosmFeed = cosmFeed;
	}
	public int getCosmDataSet() {
		return cosmDataSet;
	}
	public void setCosmDataSet(int cosmDataSet) {
		this.cosmDataSet = cosmDataSet;
	}
	public WirelessSensorConfig(String type, String iD, String niceName) {
		super();
		this.type = type;
		ID = iD;
		this.niceName = niceName;
	}
	public String getType() {
		return type;
	}
	public void setType(String type) {
		this.type = type;
	}
	public String getID() {
		return ID;
	}
	public void setID(String iD) {
		ID = iD;
	}
	
	@Override
	public String toString() {
		return "WirelessSensorConfig [type=" + type + ", ID=" + ID + ", niceName=" + niceName + ", cosmFeed=" + cosmFeed + ", cosmDataSet=" + cosmDataSet + "]";
	}
	public String getNiceName() {
		return niceName;
	}
	public void setNiceName(String niceName) {
		this.niceName = niceName;
	}
	

}
